# MixRadius v3.1 Configuration
MIXRADIUS_API_URL=http://localhost:8080/api
MIXRADIUS_API_KEY=your_mixradius_api_key_here
MIXRADIUS_ADMIN_USERNAME=admin
MIXRADIUS_ADMIN_PASSWORD=admin_password

# FreeRADIUS Database Configuration
FREERADIUS_DB_HOST=localhost
FREERADIUS_DB_PORT=3306
FREERADIUS_DB_NAME=radius
FREERADIUS_DB_USER=radius
FREERADIUS_DB_PASSWORD=radius_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# Payment Gateway Configuration
PAYMENT_GATEWAY_API_KEY=your_payment_gateway_key
PAYMENT_GATEWAY_SECRET=your_payment_gateway_secret

# Server Configuration
PORT=3000
NODE_ENV=development

# Security
SESSION_SECRET=your_session_secret_here 